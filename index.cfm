<cfset directoryPath = GetDirectoryFromPath(GetTemplatePath())>
<cfset myDirectoryListing = getDirectoryList(directoryPath)>

<!doctype html>
<html>
<head>
	<meta http-equiv="content-type" content="text/html; charset=utf-8" />
	<meta name="MSSmartTagsPreventParsing" content="TRUE" />
	<title>Directory</title>

	<style type="text/css">
		html, body { margin: 0; padding: 0;}
		body { font-family:"Trebuchet MS", verdana, sans-serif; }
		h1 { margin: 0 0 10px 0; background: black; color: white; padding: 10px; }
			h1 span { color: goldenrod; }
		table { border-collapse: collapse; margin: 10px;}
			tr:hover:not(.title) { background: #dedede; cursor: pointer; }
				td { padding:.3em; }
	</style>
</head>
<body>

<cfoutput>

	<h1>Index of <span>#getDirectoryName(directoryPath)#</span></h1>

	<table>
	<tr class="title">
		<td></td>
		<td><b>Name</b></td>
		<td><b>Size</b></td>
		<td><b>Date</b></td>
	</tr>
	<cfloop query="myDirectoryListing">
		<tr>
			<td><img src="#getIcon(name, type)#" /></td>
			<td><a href="#name#">#name#</a></td>
			<td>#getFileSize(size)#</td>
			<td>#getDate(datelastmodified)#</td>
		</tr>
	</cfloop>
</table>

</cfoutput>
</body>
</html>



<!--- 
	Don't mess with anything below this line
 --->
<cfscript>
	function getDirectoryName(path) {
		fileObj = createObject("java", "java.io.File");
		currDir = ListLast(path, fileObj.separator);
		return currDir;
	}

	function getDirectoryList(path) {
		return DirectoryList(path, false, 'query', '', 'asc');
	}

	function getDate(date) {
		return DateFormat(date,'yyyy-mm-dd') & ' ' & TimeFormat(date,'H:mm:ss');
	}

	function getFileSize(size) {
		local.newsize = ARGUMENTS.size;
		local.filetype = 'bytes';
		do {
			local.newsize = (local.newsize / 1024);
			if(local.filetype IS 'bytes') local.filetype = 'kb';
			else if(local.filetype IS 'kb') local.filetype = 'mb';
			else if(local.filetype IS 'mb') local.filetype = 'gb';
			else if(local.filetype IS 'gb') local.filetype = 'tb';
		} while((local.newsize GT 1024) AND (local.filetype IS NOT 'tb'));
		local.filesize = REMatchNoCase('[(0-9)]{0,}(\.[(0-9)]{0,2})',local.newsize);
		if (ArrayLen(local.filesize)) {
			return local.filesize[1] & '' & local.filetype;
		} else {
			return local.newsize & ' ' & local.filetype;
		}
		return local;
	}

	function getIcon(name, type) {
		// all icons are base64 encoded and completely self-contained.
		image = 'data:image/png;base64,';
		icons = {
			'.ai' = '',
			'.bmp' = '',
			'.cfm' = '',
			'.css' = '',
			'.doc' = '',
			'.html' = '',
			'.jpg' = '',
			'.mov' = '',
			'.mpg' = '',
			'.mp3' = '',
			'.pdf' = '',
			'.php' = '',
			'.png' = '',
			'.psd' = '',
			'.xls' = '',
			'.zip' = '',
			'.txt' = '',
			'.swf' = '',
			'dir' = '',
			'file' = ''
		};
		if (type EQ 'dir') {
			// it's just a directory, return the folder image
			return image & icons['dir'];
		} else {
			// get current file extension
			extension = REMatch('\.[\d\w]+$', name);
			try {
				// if the extension matches an icon return it
				return image & icons[extension[1]];
			} catch(any e) {
				// or just return a generic file icon
				return image & icons['file'];
			}
		}
	}
</cfscript>